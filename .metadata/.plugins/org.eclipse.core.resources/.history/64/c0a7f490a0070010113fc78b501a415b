package operator;

public class OperatorEx2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.println("*** 비교 연산 ***");
		int n1 = 10, n2 =20;
		System.out.println(n1 >= n2);
		System.out.println(n1 <= n2);
		System.out.println(n1 == n2);
		System.out.println(n1 != n2);
		

		
		
		
	}

}

package choice;

/*
 * 학점 계산 프로그램
 *  90 ~ 100점 : A
 *  80 ~ 89점 : B 
 *  70 ~ 79점 : C
 *  70 미만 : F  
 */
public class IfElseifElse {

	public static void main(String[] args) {
		// 다중 조건문
		int score = 96;
		char grade = ;
		
		if (score >= 90 && score <= 100) {
			grade = 'A';
		}else if (score >= 80){
			grade = 'B';
		}
    System.out.println("학점은 " + grade + "입니다.");
	}

}
